#: src/help.rs:21
msgid "    paru {-P --show}        [options]"
msgstr "    paru {-P --show}        [options]"

#: src/help.rs:22
msgid "    paru {-G --getpkgbuild} [package(s)]"
msgstr "    paru {-G --getpkgbuild} [paquet(s)]"

#: src/help.rs:24
msgid "If no arguments are provided 'paru -Syu' will be performed"
msgstr "Si aucun argument n'est fourni 'paru -Syu' sera exécuté"

#: src/help.rs:26
msgid "Options without operation:"
msgstr "Options sans opérations :"

#: src/help.rs:27
msgid "    -c --clean            Remove unneeded dependencies"
msgstr "    -c --clean            Supprime les dépendances inutiles"

#: src/help.rs:28
msgid "       --gendb            Generates development package DB used for updating"
msgstr "       --gendb            Génère la BDD de paquets de développement utilisée pour mettre à jour"

#: src/help.rs:30
msgid "New options:"
msgstr "Nouvelles options :"

#: src/help.rs:31
msgid "       --repo              Assume targets are from the repositories"
msgstr "       --repo              Suppose que les cibles sont dans les depôts"

#: src/help.rs:32
msgid "    -a --aur               Assume targets are from the AUR"
msgstr "    -a --aur               Suppose que les cibles sont dans l'AUR"

#: src/help.rs:33
msgid "    --aururl    <url>      Set an alternative AUR URL"
msgstr "    --aururl    <url>      Défini une URL alternative pour l'AUR"

#: src/help.rs:34
msgid "    --clonedir  <dir>      Directory used to download and run PKGBUILDs"
msgstr "    --clonedir  <dossier>  Dossier utilisé pour télécharger et exécuter PKGBUILDs"

#: src/help.rs:36
msgid "    --makepkg   <file>     makepkg command to use"
msgstr "    --makepkg   <fichier>      Commande makepkg à utiliser"

#: src/help.rs:37
msgid "    --mflags    <flags>    Pass arguments to makepkg"
msgstr "    --mflags    <arguments>    Passe des arguments à makepkg"

#: src/help.rs:38
msgid "    --pacman    <file>     pacman command to use"
msgstr "    --pacman    <fichier>      Commande pacman à utiliser"

#: src/help.rs:39
msgid "    --git       <file>     git command to use"
msgstr "    --git       <fichier>      Commande git à utiliser"

#: src/help.rs:40
msgid "    --gitflags  <flags>    Pass arguments to git"
msgstr "    --gitflags  <arguments>    Passe des arguments à git"

#: src/help.rs:41
msgid "    --sudo      <file>     sudo command to use"
msgstr "    --sudo      <fichier>      Commande sudo à utiliser"

#: src/help.rs:42
msgid "    --sudoflags <flags>    Pass arguments to sudo"
msgstr "    --sudoflags <arguments>    Passe des arguments à sudo"

#: src/help.rs:43
msgid "    --asp       <file>     asp command to use"
msgstr "    --asp       <fichier>      Commande asp à utiliser"

#: src/help.rs:44
msgid "    --bat       <file>     bat command to use"
msgstr "    --bat       <fichier>      Commande bat à utiliser"

#: src/help.rs:45
msgid "    --batflags  <flags>    Pass arguments to bat"
msgstr "    --batflags  <arguments>    Passe des arguments à bat"

#: src/help.rs:46
msgid "    --gpg       <file>     gpg command to use"
msgstr "    --gpg       <fichier>      Commande gpg à utiliser"

#: src/help.rs:47
msgid "    --gpgflags  <flags>    Pass arguments to gpg"
msgstr "    --gpgflags  <arguments>    Passe des arguments à gpg"

#: src/help.rs:48
msgid "    --fm        <file>     File manager to use for PKGBUILD review"
msgstr "    --fm        <fichier>      Gestionnaire de fichier à utiliser pour la relecture des PKGBUILDs"

#: src/help.rs:49
msgid "    --fmflags   <flags>    Pass arguments to file manager"
msgstr "    --fmflags   <arguments>    Passe des arguments au gestionnaire de fichier"

#: src/help.rs:51
msgid "    --completioninterval   <n> Time in days to refresh completion cache"
msgstr "    --completioninterval   <n> Temps en jour pour rafraîchir le cache de complétion"

#: src/help.rs:52
msgid "    --sortby    <field>    Sort AUR results by a specific field during search"
msgstr "    --sortby    <field>    Trier les résultats de l'AUR par un champs spécifique pendant la recherche"

#: src/help.rs:53
msgid "    --searchby  <field>    Search for packages using a specified field"
msgstr "    --searchby  <field>    Recherche des paquets en utilisant un champs spécifique"

#: src/help.rs:55
msgid "    --skipreview           Skip the review process"
msgstr "    --skipreview           Passer le processus de relecture"

#: src/help.rs:56
msgid "    --review               Don't skip the review process"
msgstr "    --review               Ne pas passer le processus de relecture"

#: src/help.rs:57
msgid "    --[no]upgrademenu      Show interactive menu to skip upgrades"
msgstr "    --[no]upgrademenu      Montrer le menu interactif pour passer les mises à jour"

#: src/help.rs:58
msgid "    --[no]removemake       Remove makedepends after install"
msgstr "    --[no]removemake       Enlève les dépendances de compilation après installation"

#: src/help.rs:59
msgid "    --[no]cleanafter       Remove package sources after install"
msgstr "    --[no]cleanafter       Enlève les paquets sources après installation"

#: src/help.rs:60
msgid "    --[no]rebuild          Always build target packages"
msgstr "    --[no]rebuild          Toujours recompiler les paquets cibles"

#: src/help.rs:61
msgid "    --[no]redownload       Always download PKGBUILDs of targets"
msgstr "    --[no]redownload       Toujours télécharger les PKGBUILDs des cibles"

#: src/help.rs:63
msgid "    --[no]pgpfetch         Prompt to import PGP keys from PKGBUILDs"
msgstr "    --[no]pgpfetch         Demande à importer les clés PGP depuis les PKGBUILDs"

#: src/help.rs:64
msgid "    --[no]useask           Automatically resolve conflicts using pacman's ask flag"
msgstr "    --[no]useask           Résoudre automatiquement les conflits en utilisant l'argument pacman ask"

#: src/help.rs:65
msgid "    --[no]newsonupgrade    Print new news during sysupgrade"
msgstr "    --[no]newsonupgrade    Affiche les dernières nouvelles pendant la mise à jour du système"

#: src/help.rs:66
msgid "    --[no]combinedupgrade  Refresh then perform the repo and AUR upgrade together"
msgstr "    --[no]combinedupgrade  Rafraîchit et effectue les mises à jour du dépôt et de l'AUR en même temps"

#: src/help.rs:67
msgid "    --[no]batchinstall     Build multiple AUR packages then install them together"
msgstr "    --[no]batchinstall     Compile plusieurs paquets depuis l'AUR et les installe en même temps"

#: src/help.rs:68
msgid "    --[no]provides         Look for matching providers when searching for packages"
msgstr "    --[no]provides         Recherche des fournisseurs correspondants lors de la recherche des paquets"

#: src/help.rs:69
msgid "    --[no]devel            Check development packages during sysupgrade"
msgstr "    --[no]devel            Vérifie les paquets de développement pendant la mise à jour du système"

#: src/help.rs:70
msgid "    --[no]installdebug     Also install debug packages when a package provides them"
msgstr "    --[no]installdebug     Installe aussi les paquets de déboguage quand un paquet les fournit"

#: src/help.rs:71
msgid "    --[no]sudoloop         Loop sudo calls in the background to avoid timeout"
msgstr "    --[no]sudoloop         Appelle sudo en arrière plan pour éviter l’expiration du délai"

#: src/help.rs:72
msgid "    --[no]chroot           Build packages in a chroot"
msgstr "    --[no]chroot           Compile les paquets dans un chroot"

#: src/help.rs:73
msgid "    --[no]sign             Sign packages with gpg"
msgstr "    --[no]sign             Signe les paquets avec gpg"

#: src/help.rs:74
msgid "    --[no]signdb           Sign databases with gpg"
msgstr "    --[no]signdb           Signe les bases de données avec gpg"

#: src/help.rs:75
msgid "    --localrepo            Build packages into a local repo"
msgstr "    --localrepo            Compile les paquets dans un dépôt local"

#: src/help.rs:76
msgid "    --nocheck              Don't resolve checkdepends or run the check function"
msgstr "    --nocheck              Ne pas résoudre les dépendances ou lancer la fonction de vérification"

#: src/help.rs:77
msgid "    --develsuffixes        Suffixes used to decide if a package is a devel package"
msgstr "    --develsuffixes        Suffixes utilisés pour décider si un paquet est un paquet de développement"

#: src/help.rs:78
msgid "    --bottomup             Shows AUR's packages first and then repository's"
msgstr "    --bottomup             Afficher les paquets de l'AUR en premier et ceux du dépôt ensuite"

#: src/help.rs:79
msgid "    --topdown              Shows repository's packages first and then AUR's"
msgstr "    --topdown              Affiche les paquets du dépôt et ceux de l'AUR ensuite"

